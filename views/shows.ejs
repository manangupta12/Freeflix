<% include ./partials/header %>
<div class="container pb-5">
    <h3 class="text-light py-5 ">Most Famous Series</h3>
    <div class="row">
        <% shows.forEach(function(show){ %>
            <div class="col-2">
                <a href="/shows/<%= show._id%>"><img src="<%= show.albumArt %>" alt="" class="img-fluid"></a>
                <p class="text-light pt-2 text-center"> <%= show.name %> (<%= show.year %>) </p>
            </div>
        <% }) %>
    </div>
</div>

<% include ./partials/footer %>